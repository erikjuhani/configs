{
  "coc.preferences.formatOnSaveFiletypes": ["ts", "tsx", "js", "jsx"],
  "languageserver": {
     "golang": {
         "command": "gopls",
         "rootPatterns": ["go.mod", ".vim/", ".git/"],
         "filetypes": ["go"],
         "initializationOptions": {
           "gocodeCompletionEnabled": true,
           "diagnosticsEnabled": true,
           "lintTool": "golint"
         }
     }
  }
}
